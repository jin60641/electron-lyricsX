{
  "appId": "com.jinsang.lyricsX",
  "extraMetadata": {
    "main": "build/electron/index.js"
  },
  "publish": {
    "provider": "github",
    "owner": "jin60641",
    "repo": "electron-lyricsX"
  },
  "win": {
    "fileAssociations": [
      {
        "ext": [
          "mp3"
        ],
        "description": "Mp3 Files"
      }
    ]
  },
  "mac": {
    "fileAssociations": [
      {
        "ext": [
          "mp3"
        ],
        "role": "Editor"
      }
    ],
    "extendInfo": {
      "LSUIElement": 1
    }
  },
  "linux": {
    "fileAssociations": [
      {
        "ext": [
          "mp3"
        ],
        "mimeType": "audio/mpeg"
      }
    ]
  },
  "directories": {
    "buildResources": "./build/"
  },
  "extraResources": {
    "from": "./scripts/",
    "to": "scripts",
    "filter": [
      "**/*"
    ]
  }
}
